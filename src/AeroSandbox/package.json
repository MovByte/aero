{
	"name": "aero-sandbox",
	"version": "0.0.2",
	"description": "The sandboxing library for aero",
	"main": "dist/sandbox.js",
	"exports": {
		".": {
			"node": {
				"import": "./extras/node/aeroSandboxPath.js",
				"require": "./extras/node/aeroSandboxPath.cjs"
			}
		},
		"./configTypes": "./types/config.d.ts",
		"./contextualIdentities": "./src/apis/contextualIdentities.ts",
		"./ElectronControlView": "./src/apis/ControlView/ElectronControlView.ts",
		"./ElectronWebView": "./src/apis/ControlView/ElectronWebView.ts"
	},
	"files": [
		"dist/*.js",
		"dist/*.map",
		"extras/**/*.js",
		"extras/**/*.cjs",
		"types/*.d.ts"
	],
	"scripts": {
		"rsdoctor": "kill $(lsof -t -i:3301) & rsdoctor analyze --profile src/AeroSandbox/dist/debug/.rsdoctor/manifest.json --port 3301",
		"check": "biome check --write",
		"format": "npm run formatBiome && npm run formatDeno",
		"formatBiome": "biome format --write",
		"formatDeno": "deno fmt",
		"serve": "rspack serve",
		"build": "node --import-tsx ./build/build.ts",
		"buildRaw": "BUILD_CONFIG_PATH=./buildConfigs/aero && rspack build",
		"initApis": "node --import=tsx ./scripts/initApis.ts",
		"initApiTypes": "node --import=tsx ./scripts/initApiTypes.ts",
		"initDist": "node --import=tsx ./scripts/InitDist.ts",
		"initGlobalsBiome": "node --import-tsx ./scripts/initGlobalsBiome.ts",
		"initGlobalsTs": "node --import-tsx ./scripts/initGlobalsTs.ts"
	},
	"author": "Ryan Wilson",
	"license": "AGPL-3.0-or-later",
	"devDependencies": {
		"@babel/core": "^7.25.8",
		"@babel/preset-env": "^7.25.8",
		"@biomejs/biome": "1.9.4",
		"@milkshakeio/webidl2ts": "^1.0.8",
		"@rsdoctor/rspack-plugin": "^0.4.7",
		"@rspack/cli": "^1.0.13",
		"@rspack/core": "^1.0.13",
		"@titaniumnetwork-dev/ultraviolet": "^3.2.7",
		"@types/webappsec-credential-management": "^0.6.8",
		"babel-loader": "^9.2.1",
		"glob": "^11.0.0",
		"navigation-api-types": "^0.5.1",
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"@mercuryworkshop/bare-as-module3": "^2.2.4",
		"@mercuryworkshop/bare-mux": "^2.1.4",
		"@rsbuild/core": "^1.0.15",
		"@types/trusted-types": "^2.0.7",
		"@types/webpack": "^5.28.5",
		"htmlparser2": "^9.1.0",
		"neverthrow": "^8.0.0",
		"traverse-the-universe": "^2.1.2",
		"zod": "^3.23.8"
	}
}
