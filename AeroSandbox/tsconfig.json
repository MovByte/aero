{
	"compilerOptions": {
		"target": "ESNext",
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"baseUrl": "./",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"sourceMap": true,
		"paths": {
			"$aero/*": ["../../*"],
			"$src/*": ["src/*"],
			"$sandbox/*": ["src/sandboxers/*"],
			"$apis/*": ["src/apis/*"],
			"$cors/*": ["src/cors/*"],
			"$fetchUtil": ["../aeroSW/src/fetchHandlers/util/*"],
			"$fetchSubsystems": ["../aeroSW/src/fetchHandlers/subsystems/*"],
			"$injPreprocessors": ["../aeroSW/src/fetchHandlers/preprocessors/*"],
			"$corsIsolation": ["../aeroSW/src/fetchHandlers/isolation/*"],
			"$util/*": ["src/util/*"],
			"$shared/*": ["src/shared/*"],
			"$types/*": ["types/*"]
		},
		"types": ["@types/websql"]
	},
	"include": [
		"rspack.config.ts",
		"src/**/*.ts",
		"types/**/*.d.ts",
		"src/defaultConfig.js",
		"../aeroSW/src/this/util/getClientURLAeroWrapper.ts",
		"../aeroSW/src/this/util/getClientURL.ts",
		"../aeroSW/types/globals.d.ts",
=	]
}
