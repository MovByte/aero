{
	"name": "aero-sandbox",
	"version": "0.0.2",
	"description": "The sandboxing library for aero",
	"homepage": "https://aero.sh",
	"main": "dist/sandbox.js",
	"exports": {
		".": {
			"node": {
				"import": "./extras/node/aeroSandboxPath.js",
				"require": "./extras/node/aeroSandboxPath.cjs"
			}
		},
		"./tests/jsdiff": "./tests/jsdiff.js",
		"./configTypes": "./types/config.d.ts",
		"./contextualIdentities": "./src/apis/contextualIdentities.ts",
		"./ElectronControlView": "./src/apis/ControlView/ElectronControlView.ts",
		"./ElectronWebView": "./src/apis/ControlView/ElectronWebView.ts"
	},
	"files": [
		"dist/*.js",
		"dist/*.map",
		"extras/**/*.js",
		"extras/**/*.cjs",
		"types/*.d.ts"
	],
	"scripts": {
		"rsdoctor": "kill $(lsof -t -i:3301) & rsdoctor analyze --profile AeroSandbox/dist/debug/.rsdoctor/manifest.json --port 3301",
		"check": "biome check --write",
		"format": "npm run formatBiome && npm run formatDeno",
		"formatBiome": "biome format --write",
		"formatDeno": "deno fmt",
		"serve": "rspack serve",
		"build": "node --no-warnings --experimental-strip-types -- ./build/build.ts",
		"buildRaw": "BUILD_CONFIG_PATH=./buildConfigs/aero && rspack build",
		"initApis": "node --import=tsx ./scripts/initApis.ts",
		"initApiTypes": "node --import=tsx ./scripts/initApiTypes.ts",
		"initDist": "node --import=tsx ./scripts/InitDist.ts",
		"init-globals:biome": "node --no-warnings --experimental-strip-types -- ./scripts/initGlobalsBiome.ts",
		"init-globals:ts": "node --no-warnings --experimental-strip-types -- ./scripts/initGlobalsTs.ts",
		"test:jsdiff": "node --no-warnings --experimental-strip-types -- ./tests/jsdiff.ts",
		"bench:encodingMethods": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(pathToFileURL(\"./tests/shared/customImportResolveHooks.mjs\"));' --no-warnings --experimental-strip-types -- ./tests/encodingMethods.ts"
	},
	"author": "Ryan Wilson",
	"license": "AGPL-3.0-or-later",
	"devDependencies": {
		"@babel/core": "^7.25.8",
		"@babel/preset-env": "^7.25.8",
		"@biomejs/biome": "1.9.4",
		"@milkshakeio/webidl2ts": "^1.0.8",
		"@minify-html/node": "^0.15.0",
		"@rsdoctor/rspack-plugin": "^0.4.7",
		"@rspack/cli": "^1.0.13",
		"@rspack/core": "^1.0.13",
		"@swc-node/register": "^1.10.9",
		"@titaniumnetwork-dev/ultraviolet": "^3.2.7",
		"@types/webappsec-credential-management": "^0.6.8",
		"babel-loader": "^9.2.1",
		"glob": "^11.0.0",
		"navigation-api-types": "^0.5.1",
		"neverthrow": "^8.0.0",
		"ts-runtime-checks": "^0.6.2",
		"typescript": "^5.6.3",
		"val-loader": "^6.0.0"
	},
	"dependencies": {
		"@mercuryworkshop/bare-as-module3": "^2.2.4",
		"@mercuryworkshop/bare-mux": "^2.1.4",
		"@rsbuild/core": "^1.0.15",
		"@swc/core": "^1.9.2",
		"@types/trusted-types": "^2.0.7",
		"@types/webpack": "^5.28.5",
		"await-async": "^0.0.1",
		"fake-indexeddb": "^6.0.0",
		"fflate": "^0.8.2",
		"htmlparser2": "^9.1.0",
		"idb": "^8.0.0",
		"tinybench": "^3.0.6",
		"typia": "^6.12.0"
	}
}
